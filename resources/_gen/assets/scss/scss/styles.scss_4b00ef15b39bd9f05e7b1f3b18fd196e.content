@import "https://fonts.googleapis.com/css2?family=Montserrat:wght@400;700&display=swap";*,*:before,*:after{box-sizing:inherit}html,body{height:100%}html{font-size:100%;-webkit-text-size-adjust:100%;-ms-text-size-adjust:100%;box-sizing:border-box;-webkit-font-smoothing:antialiased;text-rendering:optimizeLegibility;-webkit-font-smoothing:antialiased;-moz-osx-font-smoothing:grayscale;font-kerning:normal;min-height:100%}body{font-family:pt serif,Georgia,Times,times new roman,serif;font-size:16px;line-height:1.5;color:#333;margin:0;background-color:#bbcec8;opacity:.9;background-image:linear-gradient(#C6D7D1 1px,transparent 1px),linear-gradient(to right,#C6D7D1 1px,#BBCEC8 1px);background-size:20px 20px}h1{font-size:1.5em}h2{font-size:1.4em}h3{font-size:1.2em}h4{font-size:1em}h5{font-size:.85em}h6{font-size:.75em}h1 a,h2 a,h3 a,h4 a,h5 a,h6 a{text-decoration:none}h1,h2,h3,h4,h5,h6{margin-top:1.5em}a{color:#333;text-decoration:none}a:hover{text-decoration:none}blockquote{border-left:.25em solid #333;padding-left:.5em;margin-left:0}ul,ol{padding:0 0 1rem 1rem;list-style-type:square;font-variant-numeric:tabular-nums}code,pre{font-family:San Francisco Mono,Monaco,consolas,lucida console,dejavu sans mono,bitstream vera sans mono,monospace}pre,code{font-size:small}code{background-color:#333;color:#fff;padding:.2rem .4rem}pre code{display:block;border:none;background-color:#333;color:#fff;padding:1em;overflow-x:auto}img{max-width:100%}main{max-width:80ch;margin:0 auto;box-sizing:border-box;min-height:100%;background-color:#fff}footer{border-top:1px solid #0e1110;background-color:#fff;padding:2rem;max-width:80ch;margin:0 auto;color:#333}.content h1{margin-top:0}.list{display:flex;align-items:baseline;justify-content:space-between;font-size:inherit;font-weight:inherit}.list li a{white-space:nowrap;overflow:hidden;text-overflow:ellipsis;margin-right:20px}time{font-variant-numeric:tabular-nums}.parent{position:relative;z-index:1}header{background-color:#fff;padding-top:2rem}header .title{padding-left:2rem;font-family:Montserrat,sans-serif}nav{position:relative;background-color:#718b82}nav ul{list-style-type:none;padding:.5rem;margin:0;margin-top:.5rem;text-align:right}nav li{margin-left:.5em;display:inline}nav li:first-child{margin-left:0}nav a{text-decoration:none;text-transform:capitalize;color:#ffff;font-weight:700;font-family:Montserrat,sans-serif;font-size:14px}nav a:hover{text-decoration:none}.title{font-weight:700;font-size:300%;line-height:1em;text-decoration:none;color:inherit}.content{padding:2rem;word-wrap:break-word}.content p a{text-decoration:underline}tr.article-row{margin:0;padding:0}.article-title{font-family:Montserrat,sans-serif}tr.article-row a{font-family:Montserrat,sans-serif;font-size:14px}tr.article-row td.article-date{white-space:nowrap;font-family:pt serif,Georgia,Times,times new roman,serif;font-size:14px}